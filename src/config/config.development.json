{
  "environment": "development",
  "auth": {
    "enabled": false,
    "secret": ""
  },
  "info": {
    "name": "Vorba File Service (Development)"
  },
  "storage": {
    "type": "emulator",
    "local": {
      "subfolderPath": "uploads"
    },
    "azure": {
      "connectionString": "",
      "containerName": "uploads"
    },
    "emulator": {
      "connectionString": "UseDevelopmentStorage=true",
      "containerName": "uploads"
    },
    "options": {
      "safeMode": false
    }
  },
  "swagger": {
    "enabled": true,
    "title": "Vorba File Service",
    "description": "A file service supporting local and Azure Blob Storage",
    "version": "1.0",
    "path": "api"
  },
  "keyVault": {
    "vaultUrl": "https://vorba-sand-kv-2.vault.azure.net/",
    "enabled": true
  },
  "logging": {
    "level": "debug",
    "enableConsole": true
  }
}
